#%% yield
def mysum(start):
    m = start
    while True:
        a = (yield)
        if a == -1:
            break
        m += a
    yield m
iadder = mysum(0)
next(iadder)
for i in range(10):
    iadder.send(i)
print iadder.send(-1)  #: 45

