class Foo:
    def bar(self, a: int):
        print 'normal', a
    def bar(self, a: Optional[int]):
        print 'optional', a
    def bar[T](self, a: Optional[T]):
        print 'optional generic', a, a.__class__
    def bar(self, a):
        print 'generic', a, a.__class__
